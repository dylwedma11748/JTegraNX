<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.RadioMenuItem?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.effect.Reflection?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="borderPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="308.0" prefWidth="600.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jtegranx.fx.GUIController">
    <top>
        <VBox BorderPane.alignment="CENTER">
            <children>
                <MenuBar fx:id="menuBar" prefHeight="25.0" prefWidth="600.0">
                    <menus>
                        <Menu mnemonicParsing="false" text="Options">
                            <items>
                        <MenuItem mnemonicParsing="false" onAction="#clearFields" text="Clear fields" />
                        <MenuItem mnemonicParsing="false" onAction="#clearLog" text="Clear log" />
                                <RadioMenuItem fx:id="hideLog" mnemonicParsing="false" onAction="#hideLogAction" text="Hide log" />
                        <RadioMenuItem fx:id="minimizeToTray" mnemonicParsing="false" onAction="#minimizeToTrayAction" text="Minimize to tray" />
                        <MenuItem mnemonicParsing="false" onAction="#deleteSelectedConfig" text="Delete selected config" />
                            </items>
                        </Menu>
                  <Menu mnemonicParsing="false" text="Settings">
                     <items>
                        <RadioMenuItem fx:id="autoInject" mnemonicParsing="false" onAction="#autoInjectAction" text="Auto-inject" />
                     </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="Help">
                     <items>
                        <MenuItem mnemonicParsing="false" onAction="#checkForUpdates" text="Check for updates" />
                     </items>
                  </Menu>
                    </menus>
                    <effect>
                        <DropShadow />
                    </effect>
                </MenuBar>
            </children>
        </VBox>
    </top>
    <center>
        <Pane prefHeight="305.0" prefWidth="600.0" BorderPane.alignment="CENTER">
            <children>
                <VBox layoutX="38.0" layoutY="14.0" prefHeight="103.0" prefWidth="499.0" spacing="5.0">
                    <children>
                        <Label text="Payload Path:">
                            <effect>
                                <DropShadow />
                            </effect>
                        </Label>
                        <TextField fx:id="payloadPath">
                            <effect>
                                <DropShadow />
                            </effect>
                        </TextField>
                        <Label text="Arguments:">
                            <effect>
                                <DropShadow />
                            </effect>
                        </Label>
                        <TextField fx:id="arguments">
                            <effect>
                                <DropShadow />
                            </effect>
                        </TextField>
                    </children>
                </VBox>
                <Button fx:id="browse" alignment="CENTER" contentDisplay="CENTER" layoutX="546.0" layoutY="59.0" mnemonicParsing="false" onAction="#browseButtonAction" prefHeight="20.0" prefWidth="24.0" textAlignment="CENTER">
                    <effect>
                        <DropShadow />
                    </effect>
               <tooltip>
                  <Tooltip text="Browse for payload." />
               </tooltip>
                </Button>
                <TextArea fx:id="log" editable="false" layoutX="11.0" layoutY="172.0" prefHeight="100.0" prefWidth="412.0">
                    <effect>
                        <DropShadow />
                    </effect>
                </TextArea>
                <ImageView fx:id="rcmStatus" fitHeight="97.0" fitWidth="145.0" layoutX="439.0" layoutY="172.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../gui/images/init.png" />
                    </image>
               <effect>
                  <Reflection fraction="0.4" topOpacity="0.29" />
               </effect>
                </ImageView>
                <HBox fx:id="hbox" alignment="CENTER" layoutX="134.0" layoutY="127.0" prefHeight="33.0" prefWidth="305.0" spacing="13.0">
                    <children>
                        <Button fx:id="inject" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#injectAction" prefHeight="25.0" prefWidth="81.0" text="Inject" textAlignment="CENTER">
                            <effect>
                                <DropShadow />
                            </effect>
                            <tooltip>
                                <Tooltip text="Inject specified payload with specified arguments." />
                            </tooltip>
                        </Button>
                        <ComboBox fx:id="configList" onAction="#loadConfigAction" onShown="#updateConfigList" prefHeight="25.0" prefWidth="107.0" promptText="Load Config">
                            <effect>
                                <DropShadow />
                            </effect>
                     <tooltip>
                        <Tooltip text="Load a saved config." />
                     </tooltip>
                        </ComboBox>
                        <Button fx:id="saveConfig" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#saveConfigAction" prefHeight="25.0" prefWidth="81.0" text="Save Config" textAlignment="CENTER">
                            <effect>
                                <DropShadow />
                            </effect>
                     <tooltip>
                        <Tooltip fx:id="tooltip" text="Save current inout as a config." />
                     </tooltip>
                        </Button>
                    </children>
                </HBox>
                <TextField fx:id="configNameEntry" layoutX="396.0" layoutY="131.0" onKeyPressed="#configNameEntryKeyPressed" prefHeight="25.0" prefWidth="91.0" promptText="Config Name" visible="false" />
            </children>
        </Pane>
    </center>
</BorderPane>
